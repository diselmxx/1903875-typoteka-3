  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "27.4.3",
    "nodemon": "2.0.13",
    "pino-pretty": "7.3.0",
    "sqlite3": "5.0.2",
    "supertest": "6.1.6"
  },
  "dependencies": {
    "axios": "0.24.0",
    "bcrypt": "5.0.1",
    "chalk": "4.1.2",
    "concurrently": "7.2.2",
    "connect-session-sequelize": "7.1.4",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dayjs": "1.11.3",
    "dotenv": "14.2.0",
    "express": "4.17.1",
    "express-session": "1.17.3",
    "express-session-custom": "^1.1.3",
    "joi": "17.6.0",
    "multer": "1.4.2",
    "nanoid": "3.1.30",
